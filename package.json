{
  "name": "microservices-security",
  "version": "1.0.0",
  "description": "Microservices security testing project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "security:dependency": "dependency-check . --project microservices-security",
    "security:code": "eslint src/ tests/ --config .eslintrc.security.js",
    "security:secrets": "gitleaks detect --source . --config .gitleaks.toml",
    "security:vulnerability": "npm audit --audit-level=high"
  },
  "dependencies": {
    "express": "^4.18.2",
    "lodash": "4.17.15", // Intentionally vulnerable version for testing
    "mongoose": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-plugin-security": "^1.7.1",
    "@typescript-eslint/parser": "^6.7.0"
  },
  "keywords": [
    "microservices",
    "security",
    "testing"
  ],
  "author": "smc1040937705",
  "license": "MIT"
}